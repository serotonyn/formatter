{
	"name": "formatter",
	"displayName": "formatter",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:foo-lang"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "foo-lang",
				"aliases": [
					"Foo"
				],
				"extensions": [
					"foo"
				]
			}
		],
		"commands": [
			{
				"command": "extension.format-foo",
				"title": "(Bad) Format Foo Files"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"postinstall": "node ./node_modules/vscode/bin/install && tsc"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode": "^1.1.37",
		"vscode-test": "^1.5.2"
	}
}